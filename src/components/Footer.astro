---
const { title } = Astro.props;
import Logo from "../components/Logo.astro";
---

<style>
  .footer {
    flex-shrink: 0;
    display: flex;
    margin-top: 10rem;
    width: 100%;
    height: 5rem;
    background-color: #1f1f1f;
    justify-content: center;
    align-items: center;
  }

  .footer-logo {
    width: 5rem;
    height: 5rem;
  }
</style>

<footer class="footer">
  <Logo class="footer-logo" />
</footer>
