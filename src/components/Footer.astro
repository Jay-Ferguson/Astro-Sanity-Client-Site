---
const { title } = Astro.props;
const webTitle = "Wornock Cabinetry";
import { Image } from "astro:assets";
import warnockLogo from "../local-images/warnock-logo.svg";
---

<style>
  .footer {
    display: flex;
    margin-top:1rem;
    width:100%;
    height:6rem;
    background-color: #1f1f1f;
    justify-content: center;
    align-items: center;
  }

  .footer-logo {
    width: 3rem;
    height: 3rem;
  }
</style>

<footer class="footer">
  <p>
    
    <a class="link">
         <Image src={warnockLogo} alt="warnock cabinetry logo" class="footer-logo"/>
    </a>
  </p>
</footer>
